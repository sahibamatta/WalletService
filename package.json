    "react-router": "^3.2.0",
    "react-router-dom": "^4.3.1",
    "react-validation": "^3.0.7"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.10",
